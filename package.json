{
  "name": "teamwork-backend-api",
  "version": "1.0.0",
  "description": "TeamWork social application",
  "main": "src/app.js",
  "scripts": {
    "start": "set NODE_ENV= production &&  npm run build  &&  npm run db-migrate && node ./build/app.js ",
    "prestart": "npm run build",
    "prebuild": "rm -rf ./build",
    "dev": "nodemon --exec babel-node src/app.js",
    "db-migrate": "babel-node src/database/migration/tables.js",
    "pretest": "export NODE_ENV=test || SET \"NODE_ENV=test\" && npm run  db-migrate",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && nyc --reporter=html --reporter=text mocha  --require @babel/register src/**/**/*.spec.js --timeout 5000000000 --exit",
    "build": "babel  src -d ./build -s",
    "coveralls": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=lcov",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teezyfortune/TeamWork-Backend-API.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/teezyfortune/TeamWork-Backend-API/issues"
  },
  "homepage": "https://github.com/teezyfortune/TeamWork-Backend-API#readme",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@hapi/joi": "^16.1.8",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.21.0",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "faker": "^4.1.0",
    "heroku-cli": "^7.0.9",
    "jsonwebtoken": "^8.5.1",
    "logger": "0.0.1",
    "make-runnable": "^1.3.6",
    "mocha": "^6.2.3",
    "multiparty": "^4.2.1",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "pg": "^7.18.2",
    "swagger-jsdoc": "^3.7.0",
    "swagger-ui-express": "^4.1.4",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "install": "^0.13.0",
    "mocha-lcov-reporter": "^1.3.0",
    "npm": "^6.14.4",
    "prettier": "^1.19.1"
  }
}
